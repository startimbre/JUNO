package com.juno.shopping.biz.product;

import java.util.Date;

public class ProductVO {
	
	private String id;
	private String storageCode;
	private String typeCode;
	private String krName;
	private String jpName;
	private String companyName;
	private int price;
	private int primeCost;
	private int pointPercent;
	private int quantity;
	private Date orderDate;
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getStorageCode() {
		return storageCode;
	}
	public void setStorageCode(String storageCode) {
		this.storageCode = storageCode;
	}
	public String getTypeCode() {
		return typeCode;
	}
	public void setTypeCode(String typeCode) {
		this.typeCode = typeCode;
	}
	public String getKrName() {
		return krName;
	}
	public void setKrName(String krName) {
		this.krName = krName;
	}
	public String getJpName() {
		return jpName;
	}
	public void setJpName(String jpName) {
		this.jpName = jpName;
	}
	public String getCompanyName() {
		return companyName;
	}
	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public int getPrime_cost() {
		return prime_cost;
	}
	public void setPrime_cost(int prime_cost) {
		this.prime_cost = prime_cost;
	}
	public int getQuantity() {
		return quantity;
	}
	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}
	public Date getOrder_date() {
		return order_date;
	}
	public void setOrder_date(Date order_date) {
		this.order_date = order_date;
	}
	@Override
	public String toString() {
		return "ProductVO [id=" + id + ", storageCode=" + storageCode + ", typeCode=" + typeCode + ", krName=" + krName
				+ ", jpName=" + jpName + ", companyName=" + companyName + ", price=" + price + ", prime_cost="
				+ prime_cost + ", quantity=" + quantity + ", order_date=" + order_date + "]";
	}
}
